	&lt;tr&gt;&lt;td&gt;&lt;a href="/var/knowledge/storage/images/media/images/efard_logo/714317-1-eng-GB/efard_logo.png" rel="lightbox[]" &gt;	&lt;img border="0" src="/var/knowledge/storage/images/media/images/efard_logo/714317-1-eng-GB/efard_logo_medium.png" width="171" height="98"&gt;
	  &lt;/a&gt;&lt;/td&gt;
	&lt;tr&gt;
		&lt;td&gt;&lt;/td&gt;
			&lt;/tr&gt;
	&lt;/table&gt;
  	
